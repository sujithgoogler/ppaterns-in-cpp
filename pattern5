#include<bits/stdc++.h>
using namespace std;
#define endl "\n"
/*   *
     **
     ***
     ****
     *****
     ****
     ***
     **
     *

*/    
void pattern5(int n)
{
	for(int row=1;row<=2*n;row++)
	{
		int tcr=row>n?2*n-row:row;
		for(int col=1;col<tcr;col++)
		{
             cout<<"*";
			
		}
		cout<<endl;
	}
}
int main()
{
	int n;
	cin>>n;
	pattern5(n);
	return 0;
}
